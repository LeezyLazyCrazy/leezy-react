{
    "name": "Leezy React Document",
    "tsconfig": "tsconfig.json",
    "entryPointStrategy": "expand",
    "plugin": [
        "typedoc-theme-hierarchy",
        "typedoc-plugin-mermaid"
    ],
    "theme": "hierarchy",
    "emit": "both",
    "includeVersion": true,
    "entryPoints": [
        "src"
    ],
    "out": "docs"
}